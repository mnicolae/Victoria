package ca.utoronto.csc301.SelfCheckOut.Exceptions;

/**
 * Thrown when User enters a negative value as payment amount
 */
public class NegativePayAmountException extends Exception {
	private static final long serialVersionUID = 1L;

	public NegativePayAmountException() {
	}

	public NegativePayAmountException(String msg) {
		super(msg);
	}

	public NegativePayAmountException(Throwable cause) {
		super(cause);
	}

	public NegativePayAmountException(String msg, Throwable cause) {
		super(msg, cause);
	}

}
